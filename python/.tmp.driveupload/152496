# 연습문제 1

nums = [ 1 , 2 , 3 ]
print(nums)

fruits = []
print(fruits)
fruits.append('자몽')
print(fruits)
fruits.append('멜론')
print(fruits)
fruits.append('레몬')
print(fruits)

print(fruits[1])

print(fruits)
fruits.remove('자몽')
print(fruits)

nums = ( 1 , 2 , 3 )
print(nums)

my_tuple = ( 3.14 , 2.71 )
print(my_tuple)
print(my_tuple[1])