import re

#match 객체 (리턴값) 메서드

p = re.compile('[a-z]+')
m = p.match('python')
m.group()
print(m.group())
m.start()
print(m.start())
m.end()
print(m.end())
m.span()
print(m.span())

p = re.compile('[a-z]+')
m = p.search('python')
m.group()
print(m.group())
m.start()
print(m.start())
m.end()
print(m.end())
m.span()
print(m.span())

# 축약 형태

m = re.match('[a-z]+','python')
m.group()
print(m.group())
m.start()
print(m.start())
m.end()
print(m.end())
m.span()
print(m.span())