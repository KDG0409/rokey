class Phone :
    def __init__(self,number,color) :
        self.number = number
        self.color = color
        
class SmartPhone(Phone):
    def __init__(self,number,color,company) :
        super().__init__(number,color)
        self.company = company 



apple = SmartPhone("010-1234-5678", "검정", "애플")
print(apple.number)
print(apple.color)
print(apple.company)

